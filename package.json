{
  "name": "discord-llm-bot",
  "version": "1.0.0",
  "description": "A Discord bot to communicate with LLMs and execute Python code",
  "type": "commonjs",
  "scripts": {
    "lint": "npx eslint src/",
    "check-types": "tsc --noEmit",
    "version": "tsc -v",
    "build": "tsc && mkdir -p dist/scripts && cp src/scripts/* dist/scripts/ || true",
    "start": "node --max-old-space-size=256 dist/src/index.js",
    "start:dev": "nodemon --exec npx ts-node src/index.ts",
    "validate": "npm-run-all --parallel lint test",
    "test": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test jest --runInBand",
    "test:coverage": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test jest --coverage --runInBand",
    "test:debug": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand",
    "test:monitor": "npx cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand --watch",
    "ncu": "ncu"
  },
  "_moduleAliases": {
    "@command": "dist/src/command",
    "@common": "dist/src/common",
    "@config": "dist/src/common/config",
    "@llm": "dist/src/llm",
    "@message": "dist/src/message",
    "@discord": "dist/src/message/discord",
    "@utils": "dist/src/utils",
    "@types": "dist/src/types",
    "@src": "dist/src"
  },
  "keywords": [],
  "author": "matthewhand+dot+au+at+gmail+dot+com",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "@babel/traverse": "^7.24.7",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^2.0.0",
    "@discordjs/voice": "^0.17.0",
    "@eslint/config-array": "^0.17.1",
    "@eslint/object-schema": "^2.1.4",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/pino": "^7.0.4",
    "abstract-syntax-tree": "^2.22.0",
    "are-we-there-yet": "^3.0.1",
    "axios": "^1.6.5",
    "config": "^3.3.12",
    "debug": "^4.3.6",
    "discord-api-types": "^0.37.67",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "form-data": "^4.0.0",
    "gauge": "^4.0.4",
    "glob": "^9.3.5",
    "gpt-tokenizer": "^2.1.2",
    "libsodium-wrappers": "^0.7.9",
    "lru-cache": "^11.0.0",
    "node-fetch": "^2.6.1",
    "npmlog": "^6.0.2",
    "openai": "^4.28.4",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "rimraf": "^4.4.1",
    "typeforce": "^1.18.0",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.1",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-flow": "^7.24.1",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@types/config": "^3.3.4",
    "@types/debug": "^4.1.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/ffmpeg-static": "^3.0.3",
    "@types/form-data": "^2.2.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^22.5.0",
    "@types/node-fetch": "^2.6.11",
    "@types/npmlog": "^7.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/uuidv4": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4",
    "wrangler": "^3.23.0"
  }
}
